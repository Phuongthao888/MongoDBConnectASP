@model Mong.Models.svMo

@{
    ViewData["Title"] = "Details";
}

<h3>Thông tin chi tiết</h3>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            Tên sinh viên
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            Địa chỉ tạm trú
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.address)
        </dd>
        <dt class = "col-sm-2">
            Gmail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.gmail)
        </dd>
        <dt class = "col-sm-2">
            Số điện thoại
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.sdt)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Chỉnh sửa", "Edit", new {  id = Model.Id  }) |
    @Html.ActionLink("Xóa", "Delete", new {  id = Model.Id  }) |
    <a asp-action="Index">Quay lại</a>
</div>
